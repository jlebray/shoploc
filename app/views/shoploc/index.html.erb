<h1>Shoploc#index</h1>
<%= bootstrap_form_tag do |f| %>
  <%= f.number_field :latitude, step: 'any' %>
  <%= f.number_field :longitude, step: 'any' %>
  <%= f.number_field :radius, step: 'any' %>
  <%= f.form_group do %>
    <%= f.submit %>
  <% end %>
<% end %>
